{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"paths": {
			"#/*": [
				"src/components/*"
			],
			"#utils/*": [
				"src/shared/utils/*"
			],
			"#api/*": [
				"src/shared/api/*"
			],
			"#stores/*": [
				"src/contexts/*"
			],
			"#data/*": [
				"src/shared/data/*"
			],
			"#mocks/*": [
				"src/shared/mocks/*"
			],
			"#types": [
				"src/shared/typings.d.ts"
			],
		},
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": false,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
	},
	"include": [
		"**/*.ts",
		"**/*.tsx"
	],
}