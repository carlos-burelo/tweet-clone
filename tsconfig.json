{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"paths": {
			"#components/*": [
				"src/components/*"
			],
			"#utils/*": [
				"src/utils/*"
			],
			"#shared/*": [
				"src/shared/*"
			],
			"#hooks/*": [
				"src/hooks/*"
			],
			"#stores/*": [
				"src/contexts/*"
			],
			"#data/*": [
				"src/shared/data/*"
			],
			"#mocks/*": [
				"src/shared/data/mocks/*"
			],
			"#types": [
				"src/shared/types/index"
			],
		},
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true
	}
}